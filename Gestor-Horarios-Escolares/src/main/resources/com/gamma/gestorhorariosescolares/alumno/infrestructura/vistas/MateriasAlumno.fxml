<?import java.lang.String?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="550.0" prefWidth="637.0" xmlns="http://javafx.com/javafx/8">
    <children>
        <Label prefHeight="40" text="Materias" AnchorPane.leftAnchor="30.0" AnchorPane.rightAnchor="30.0"
               AnchorPane.topAnchor="20.0">
            <styleClass>
                <String fx:value="h2"/>
                <String fx:value="b"/>
            </styleClass>
        </Label>

        <Label styleClass="h5" text="Periodo escolar: " AnchorPane.leftAnchor="50" AnchorPane.topAnchor="80"/>

        <TableView fx:id="tablaMateriasAlumno" styleClass="h5" AnchorPane.bottomAnchor="20" AnchorPane.leftAnchor="30"
                   AnchorPane.rightAnchor="30" AnchorPane.topAnchor="130">

        </TableView>
        <ComboBox fx:id="cbxPeriodoEscolar" layoutX="169.0" layoutY="75.0" prefHeight="31.0" prefWidth="259.0"
                  promptText="Selecciona un periodo escolar" styleClass="h5"/>
    </children>


</AnchorPane>
